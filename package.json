{
  "name": "ai-pixel-tracker-client",
  "version": "1.0.0",
  "description": "JavaScript tracking pixel that detects AI bot visits - Client SDK",
  "main": "ai-pixel-tracker.js",
  "types": "ai-pixel-tracker.d.ts",
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "watch": "webpack --mode=development --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "size": "bundlesize",
    "serve": "http-server dist -p 8080",
    "minify": "terser ai-pixel-tracker.js -o ai-pixel-tracker.min.js --compress --mangle"
  },
  "keywords": [
    "tracking-pixel",
    "ai-detection",
    "bot-detection",
    "web-analytics",
    "javascript-sdk",
    "tracking-script",
    "chatgpt",
    "claude",
    "perplexity",
    "ai-bots"
  ],
  "author": "AI Pixel Tracker",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/ai-pixel-tracker.git",
    "directory": "client"
  },
  "bugs": {
    "url": "https://github.com/your-username/ai-pixel-tracker/issues"
  },
  "homepage": "https://ai-pixel.com",
  "files": [
    "ai-pixel-tracker.js",
    "ai-pixel-tracker.min.js",
    "ai-pixel-tracker.d.ts",
    "README.md"
  ],
  "devDependencies": {
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "terser-webpack-plugin": "^5.3.9",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "bundlesize": "^0.18.1",
    "http-server": "^14.1.1",
    "terser": "^5.26.0",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.3"
  },
  "bundlesize": [
    {
      "path": "./ai-pixel-tracker.min.js",
      "maxSize": "10 kB",
      "compression": "gzip"
    }
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "jsdelivr": "ai-pixel-tracker.min.js",
  "unpkg": "ai-pixel-tracker.min.js"
}